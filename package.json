{
  "name": "apollo",
  "description": "The Kore's robot child",
  "private": true,
  "version": "1.0.0",
  "author": "Andr√© 'Dezzy' <@andredezzy>",
  "homepage": "https://apollo.paymetrade.com",
  "license": "MIT",
  "main": "./dist/main.js",
  "engines": {
    "node": ">= 12.16.x",
    "yarn": ">= 1.22.x",
    "npm": ">= 6.14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaymeTrade/apollo.git"
  },
  "bugs": {
    "url": "https://github.com/PaymeTrade/apollo/issues"
  },
  "scripts": {
    "lint": "eslint src electron --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "build": "rimraf dist && npm-run-all -p build:*",
    "build:electron": "cross-env NODE_ENV=production webpack --config webpack/electron.webpack.js",
    "build:react": "cross-env NODE_ENV=production webpack --config webpack/react.webpack.js",
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack/react.webpack.js",
    "start": "npm-run-all -p start:react electron:serve",
    "start:react": "cross-env NODE_ENV=production webpack-dev-server --config webpack/react.webpack.js",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "disabled:postinstall": "electron-builder install-app-deps",
    "package": "yarn build && electron-builder build --publish never",
    "test": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "postinstall": "yalc update && husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "build": {
    "appId": "com.paymetrade.apollo",
    "productName": "Apollo",
    "files": [
      "dist/"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    }
  },
  "dependencies": {
    "@hemes/core": "file:.yalc/@hemes/core",
    "@hemes/iqoption": "file:.yalc/@hemes/iqoption",
    "@tippyjs/react": "^4.2.0",
    "@types/react-transition-group": "^4.4.0",
    "@unform/core": "^2.1.3",
    "@unform/web": "^2.1.3",
    "axios": "^0.21.0",
    "cors-anywhere": "^0.4.4",
    "date-fns": "^2.17.0",
    "date-fns-tz": "^1.3.3",
    "dotenv": "^8.2.0",
    "electron-store": "^6.0.1",
    "electron-updater": "^5.0.1",
    "framer-motion": "^3.3.0",
    "i18next": "^19.8.4",
    "i18next-electron-language-detector": "^0.0.10",
    "i18next-fs-backend": "^1.0.5",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "polished": "^4.0.5",
    "react": "^17.0.1",
    "react-contextmenu": "^2.14.0",
    "react-currency-input": "^1.3.6",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.4",
    "react-icons": "^4.1.0",
    "react-loading-skeleton": "^2.2.0",
    "react-modal": "^3.12.1",
    "react-number-format": "^4.4.4",
    "react-resizable": "^1.11.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.1.0",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "react-virtual": "^2.3.2",
    "recoil": "^0.1.2",
    "socket.io-client": "2.3.0",
    "styled-components": "^5.2.1",
    "tippy.js": "^6.2.7",
    "uuid": "^8.3.1",
    "wait-on": "^5.2.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/plugin-transform-runtime": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/runtime": "7.10.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/react": "^11.2.2",
    "@types/i18next-fs-backend": "^1.0.0",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-modal": "^3.10.6",
    "@types/react-resizable": "^1.7.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.13",
    "@types/socket.io-client": "^1.4.35",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^11.0.3",
    "electron-builder": "^23.0.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "file-loader": "^6.2.0",
    "git-branch-is": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "json-schema-typed": "^7.0.3",
    "lerna": "^3.22.1",
    "lint-staged": "10.4.0",
    "prettier": "^2.0.5",
    "react-refresh": "^0.9.0",
    "rimraf": "3.0.2",
    "shx": "^0.3.3",
    "sort-package-json": "^1.48.0",
    "ts-jest": "26.4.4",
    "ts-node-dev": "^1.0.0",
    "type-fest": "^0.21.1",
    "typescript": "^4.1.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "yalc": "^1.0.0-pre.53"
  }
}
